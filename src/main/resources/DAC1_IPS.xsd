	<!-- IPType_Type -->
	<xs:simpleType name="IPType_Type">
		<xs:annotation>
			<xs:documentation>This data type describes the type of IP.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Land">
				<xs:annotation>
					<xs:documentation>Land</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Building">
				<xs:annotation>
					<xs:documentation>Building</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LandAndBuilding">
				<xs:annotation>
					<xs:documentation>Land + Building</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BuildingUnit">
				<xs:annotation>
					<xs:documentation>Building Unit (e.g. apartment)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LandAndBuildingUnit">
				<xs:annotation>
					<xs:documentation>Building Unit + Land (e.g. apartment with a portion in the soil)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Shares">
				<xs:annotation>
					<xs:documentation>Shares (this type is very specific to some countries (e.g. Finland) where a building is officially owned by a company but the shares are associated to specific building units and shareholders have a direct right on their unit(s) and will accordingly for instance directly rent the building under their own name and not under the company’s name)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Unknown">
				<xs:annotation>
					<xs:documentation>Unknown</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
